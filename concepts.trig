prefix dct:      <http://purl.org/dc/terms/>
prefix geo:      <http://www.opengis.net/ont/geosparql#>
prefix owl:      <http://www.w3.org/2002/07/owl#>
prefix prov:     <http://www.w3.org/ns/prov#>
prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#>
prefix rec28:    <https://service.unece.org/trade/uncefact/vocabulary/rec28#>
prefix sh:       <http://www.w3.org/ns/shacl#>
prefix skos:     <http://www.w3.org/2004/02/skos/core#>
prefix uncefact: <https://service.unece.org/trade/uncefact/trade/uncefact/vocabulary/uncefact#>
prefix uncl3035: <https://service.unece.org/trade/uncefact/vocabulary/uncl3035#>

prefix con:      <https://logistiek.triply.cc/TopsectorLogistiek/bdi-ontology/model/con/>
prefix graph:    <https://logistiek.triply.cc/TopsectorLogistiek/bdi-ontology/graph/>
prefix scheme:   <https://logistiek.triply.cc/TopsectorLogistiek/bdi-ontology/model/scheme/>

graph:concepts {
  con:heeftAankomstpunt a rdf:Property.

  con:heeftKoper
    a rdf:Property;
    skos:related uncefact:buyerParty.

  con:heeftVerkoper
    a rdf:Property;
    skos:related uncefact:sellerParty.

  con:heeftVertrekpunt a rdf:Property.
  # 'skos:related uncefact:departurePoint' is not the property that we want
  # to use, since this is a text field.

  con:isEen a rdf:Property.

  con:maaktOnderdeelUitVan a rdf:Property.

  con:operatorParty
    a rdf:Property;
    skos:related uncefact:operatorParty.

  con:Aankomsttijd_gepland
    a skos:Concept;
    con:maaktOnderdeelUitVan con:Itinerary.

  con:Aankomsttijd_verwacht
    a skos:Concept;
    con:maaktOnderdeelUitVan con:Itinerary.

  con:Afleverinformatie
    a skos:Concept;
    skos:related uncefact:Instructions.

  con:Aflevertijdvenster
    a skos:Concept;
    skos:related uncefact:Regulatory.

  con:Chaufeur
    a skos:Concept;
    con:isEen con:Party.

  con:Gemeente
    a skos:Concept;
    con:isEen con:Party.

  con:Itinerary
    a skos:Concept;
    skos:related uncefact:Itinerary.

  con:Kenteken
    a skos:Concept;
    skos:related uncefact:TransportMeans.#identifier

  con:Leverancier
    a skos:Concept;
    con:isEen con:Party.

  con:Locatie
    a skos:Concept;
    skos:related uncefact:Location.

  con:Order
    a skos:Concept;
    con:buyer con:Party;
    con:seller con:Party;
    skos:related uncefact:TradeAgreement.

  con:Party
    a skos:Concept;
    con:heeftLocatie con:Locatie;
    skos:related uncefact:Party.

  con:Planner
    a skos:Concept;
    con:isEen con:Party.

  con:Poort
    a skos:Concept;
    con:isEen con:Locatie.

  con:Restricties
    a skos:Concept;
    skos:related uncefact:Instructions.

  con:Route
    a skos:Concept;
    con:heeftAankomstpunt con:Locatie;
    con:heeftVertrekpunt con:Locatie;
    skos:related uncefact:Route.

  con:Stad
    a skos:Concept;
    con:isEen con:Locatie.

  con:Toegangscode
    a skos:Concept;
    skos:related uncefact:Seal.

  con:Transporteur
    a skos:Concept;
    con:isEen con:Party.

  con:TransporteurNaam
    a skos:Concept.

  con:Venstertijden
    a skos:Concept.

  con:Wagentype
    a skos:Concept;
    con:operatorParty con:Party;
    skos:related uncefact:TransportMeans. # 'uncefact:Vehicle' is too detailed for our purposes.

  con:ZeroEmissieZone
    a skos:Concept;
    skos:related uncefact:GeographicUsageLimit.
}
